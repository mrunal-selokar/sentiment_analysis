<%= form_for Hotel.new do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

 <%= f.label :name %>
  <%= f.text_field :name, class: 'form-control' %>

  <%= f.label :city %>
  <%= f.text_field :city, class: 'form-control' %>

  <%= f.submit "Add", class: "btn btn-success" %>
<% end %>
