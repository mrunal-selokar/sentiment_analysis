<% provide(:title, 'All users') %>
<div class="jumbotron text-center">
<h1>All users</h1>
</div>

<%= will_paginate %>

<table class="table table-hover">
	<thead>
		<tr>
			<th>Name</th>
			<th>Email</th>
			<th>Reviews</th>
			<th> </th>
		</tr>
	</thead>
	<tbody>
		<%= render @users %>
	</tbody>
</table>

<%= will_paginate %>